{
    "name": "@ulaval/modul-vuex",
    "version": "1.0.1-0",
    "description": "MODUL vuex utilities",
    "author": "Martin Simard <martin.simard@dti.ulaval.ca>",
    "private": false,
    "scripts": {
        "clean": "rimraf dist",
        "reset": "rmdir /S /Q node_modules",
        "build": "rimraf dist && tsc",
        "tslint": "tslint -c conf/tslint.json -e 'src/**/*.d.ts' -e 'tests/unit/**/*.d.ts' -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/e2e/*.ts'",
        "prepack": "npm run clean && npm run build",
        "component": "node ./tests/meta/component-generator"
    },
    "dependencies": {},
    "peerDependencies": {
        "vue": "^2.5.9"
    },
    "devDependencies": {
        "rimraf": "^2.6.2",
        "typescript": "^2.9.2",
        "vue": "^2.5.16",
        "vuex": "^3.0.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ulaval/modul-vuex.git"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 6.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "files": [
        "dist"
    ],
    "license": "Apache-2.0"
}
